@model Compiler.Models.DealWithUI
@{
    ViewData["Title"] = "Scanning result";
}
<div class="container row">
    <div class="text-white" style="background-color:black; font-family:'Courier New'; padding:10px">
        @if (Model != null)
        {
            foreach (var token in Model.Tokens)
            {
                <p>Line: @token.line</p>
                <p>Token text: @token.keyword</p>
                <p>Token type: @token.returnToken</p>
                <hr />
            }
            foreach (var error in Model.Errors)
            {
                <p>Line: @error.line</p>
                <p>Token text: @error.keyword</p>
                <hr />
            }
            <p>Errors NO: @Model.nOfErrors</p>
        }
    </div>
</div>